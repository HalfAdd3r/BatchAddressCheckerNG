{
	"name": "Batch Address Checker",
	"description": "Batch Address verification app using FedEx Web Services",
	"version": "0.1",
	"app":{
		"background":{
			"scripts": ["js/background.js"]
		}
	},
	"icons":{
		"16": "img/BatchAddressCheck.png",
		"128": "img/BatchAddressCheck.png",
		"source": "http://www.graphicsfuel.com/"
	},
	"permissions": [
    	"http://www.google.com/",
    	"https://wsbeta.fedex.com:443/web-services/*",
    	"https://code.jquery.com/*",
    	"storage",
    	{"fileSystem": ["write", "directory"]} 
  	]
}
{
	"name": "Batch Address Checker",
	"description": "Batch Address verification app using FedEx Web Services",
	"version": "0.1",
	"app":{
		"background":{
			"scripts": ["js/background.js"]
		}
	},
	"icons":{
		"96": "img/RoundIcons-Free-Set-31-96.png",
		"128": "img/RoundIcons-Free-Set-31-128.png",
		"source": "http://www.smashingmagazine.com/2014/01/24/roundicons-set-60-icons-png-ai-source/"
	},
	"permissions": [
    	"http://www.google.com/",
    	"https://ws.fedex.com:443/web-services/*",
    	"https://code.jquery.com/*",
    	"storage",
    	{"fileSystem": ["write", "directory"]} 
  	]
}